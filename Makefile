all: valid invalid

valid:
	# These should run just fine.
	-python3 main.py $(ARGS) "1+1"
	-python3 main.py $(ARGS) "1 + 1"
	-python3 main.py $(ARGS) " 1 + 1 "
	-python3 main.py $(ARGS) " 1 - 1 "
	-python3 main.py $(ARGS) " 1 - 2 "
	-python3 main.py $(ARGS) "          1     -           2            "
	-python3 main.py $(ARGS) "          1     -2                  "
	-python3 main.py $(ARGS) "          1-           2              "
	-python3 main.py $(ARGS) " 10 + 1 - 10"
	-python3 main.py $(ARGS) " 10 - 10 + 100 - 100 + 1000 - 1000"
	-python3 main.py $(ARGS) "1*1"
	-python3 main.py $(ARGS) "1  *  1"
	-python3 main.py $(ARGS) "1+1  *  1"
	-python3 main.py $(ARGS) "1/1"
	-python3 main.py $(ARGS) "1  /  1"
	-python3 main.py $(ARGS) "1+1  /  1"
	-python3 main.py $(ARGS) "2*2/4+1"
	-python3 main.py $(ARGS) "2 * 2 / 4 + 1"
	-python3 main.py $(ARGS) "     2 * 2 / 4 + 1       "
	-python3 main.py $(ARGS) "2*2*2*2*2+1"
	-python3 main.py $(ARGS) "2*2*2*2*2+1+2*2*2*2*2"
	-python3 main.py $(ARGS) "2*2*2*2*2+1+2*2*2*2*2+1+2*2*2*2*2"
	-python3 main.py $(ARGS) "32/2/2/2/2/2+1"
	-python3 main.py $(ARGS) "32/2/2/2/2/2+1+32/2/2/2/2/2"
	-python3 main.py $(ARGS) "32/2/2/2/2/2+1+32/2/2/2/2/2+1+32/2/2/2/2/2"

invalid:
	# These should throw some kind of error.
	-python3 main.py $(ARGS) "+"
	-python3 main.py $(ARGS) "-"
	-python3 main.py $(ARGS) "A"
	-python3 main.py $(ARGS) "1A+1B"
	-python3 main.py $(ARGS) "+1"
	-python3 main.py $(ARGS) "1-"
	-python3 main.py $(ARGS) ""
	-python3 main.py $(ARGS) "1 1"
	-python3 main.py $(ARGS) "1 10"
	-python3 main.py $(ARGS) "10 10"
	-python3 main.py $(ARGS) "10 1"
	-python3 main.py $(ARGS) "+1 1"
	-python3 main.py $(ARGS) "1 1-"
	-python3 main.py $(ARGS) "1 1+1"
	-python3 main.py $(ARGS) "1 1++1"
	-python3 main.py $(ARGS) "1 1--1"
	-python3 main.py $(ARGS) "1--1"
